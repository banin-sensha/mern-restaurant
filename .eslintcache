 {\n        case 'updateCount': {\n            let arr = (state || []).map((item) => {\n                if (item._id === action.food._id) {\n                    return action.food;\n                }\n                else {\n                    return item;\n                }\n            });\n\n            return arr;\n        }\n        \n        case 'initialize':\n            return [...state, action.food];\n\n        default:\n            return state;\n    }\n}\n\nconst FoodItems = ({setCartItems, categoryList, divRef}) => {\n    const [foodItems, setFoodItems] = useState([]);\n\n    const [foodItemsWithCount, dispatch] = useReducer(reducer, []);\n\n    const dispatchAction = (action) => {\n        dispatch(action);\n    }\n    \n    useEffect(() => {\n        axios.get(\"http://localhost:5000/foods/\")\n            .then(res => {\n                setFoodItems(res.data);\n            })\n    }, []);\n\n    useEffect(() => {\n        let filteredCartItems = foodItemsWithCount.filter((item) => item.count > 0);\n        setCartItems(filteredCartItems);\n    }, [foodItemsWithCount]);\n\n    var groupedFoodItems = _.groupBy(foodItems, \"category_id\");\n\n    return (\n        <div className=\"food-list pl-20x mt-20x\">\n            {\n                (categoryList || []).map((item, index) => {\n                    let categorizedFoodItem = _.find(groupedFoodItems, (value, key) => {\n                        if (key == item._id) return value;\n                    });\n\n                    return (\n                        <div key={index} ref={el => divRef.current[index] = el}>\n                            <div className=\"header-food flex flex-center pt-10x pb-10x mb-15x\">\n                                {item.name}\n                            </div>\n                            {\n                                (categorizedFoodItem || []).map((food, index) => (\n                                    <div className=\"food-item flex flex-between mt-20x pb-10x\" key={index}>\n                                        <FoodItem food={food} />\n                                        <AddOrDeleteItem food={food} dispatchAction={dispatchAction} />\n                                    </div>\n                                ))\n                            }\n                        </div>\n                        \n                    )\n                })\n            }\n        </div>\n    );\n}\n\nexport default FoodItems;","/Users/baninsenshashrestha/Banin/projects/menu/src/components/FoodItem.js",[],"/Users/baninsenshashrestha/Banin/projects/menu/src/components/AddOrDeleteItem.js",["69","70"],"import { faMinus, faPlus } from '@fortawesome/free-solid-svg-icons';\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\nimport React, { useEffect, useState} from 'react';\nimport Beef from '../images/beef.jpg';\n\nconst AddOrDeleteItem = ({ food, dispatchAction}) => {\n    const [count, setCount] = useState(0);\n    const [foodWithCount, setFoodWithCount] =  useState({...food, count: count});\n\n    const addItem = (e) => {\n        e.preventDefault();\n        setCount(count + 1);\n        setFoodWithCount({ ...foodWithCount, count: count + 1});\n    }\n\n    const deleteItem = (e) => {\n        e.preventDefault();\n        if (count > 0) {\n            setCount(count - 1);\n            setFoodWithCount({ ...foodWithCount, count: count - 1 });\n        }\n    }\n\n    useEffect(() => {\n        dispatchAction({\n            type: 'updateCount',\n            food: foodWithCount\n        });\n\n    }, [foodWithCount]);\n\n    useEffect(() => {\n        dispatchAction({\n            type: 'initialize',\n            food: foodWithCount\n        })\n    }, []);\n\n    return (\n        <div>\n            <div>\n                <img src={Beef} alt=\"Please click on Button to add or delete\" height=\"60px\" />\n            </div>\n            <div className=\"add-or-delete\">\n                {\n                    count === 0 &&\n                    <div className=\"add-btn flex flex-center curP font-bold\" onClick={addItem}>Add</div>\n                }\n                {\n                    count > 0 &&\n                    <span className=\"flex flex-between\">\n                        <FontAwesomeIcon className=\"pl-3x curP\" icon={faMinus} onClick={deleteItem} />\n                        {count}\n                        <FontAwesomeIcon className=\"pr-3x curP\" icon={faPlus} onClick={addItem} />\n                    </span>\n                }\n            </div>\n        </div>\n    );\n}\n\nexport default AddOrDeleteItem;","/Users/baninsenshashrestha/Banin/projects/menu/src/components/CartItems.js",[],"/Users/baninsenshashrestha/Banin/projects/menu/src/components/EmptyCart.js",[],"/Users/baninsenshashrestha/Banin/projects/menu/src/components/NonEmptyCart.js",["71"],"import React, { useEffect, useState } from 'react';\nimport _ from 'lodash';\nimport CartItem from './CartItem';\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\nimport { faRupeeSign } from '@fortawesome/free-solid-svg-icons';\nimport {Link} from 'react-router-dom';\n\n\nconst NonEmptyCart = ({cartItems}) => {\n    const [totalItems, setTotalItems] = useState(0);\n\n    useEffect(() => {\n        let count = _.reduce(cartItems, (result, item) => result + item.count, 0);\n        setTotalItems(count);\n\n    }, [cartItems]);\n\n    const getSubtotal = () => {\n        let subtotal = _.reduce(cartItems, (result, item) => {\n            result = result + item.count * item.price;\n            return result; \n        }, 0);\n\n        return subtotal;\n    };\n\n    return (\n        <div className=\"cart-container\">\n            <div className=\"font-bold fs-30 mb-10x\">\n                Cart\n            </div>\n            <div className=\"font-medium mb-15x\">\n                {\n                    totalItems == 1 &&\n                    totalItems + \" ITEM\"\n                }\n                {