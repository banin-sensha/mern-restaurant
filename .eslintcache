reventDefault();\n        setTextSelected({[index]: true}); \n        divRef.current[index].scrollIntoView({\n            behavior: 'smooth',\n            block: 'start',\n        });\n    }\n\n    return (\n        <div className=\"ml-20x mt-10x pr-30x\">\n            {\n                (categoryList || []).map((category, index) => {\n                    return (\n                        <div\n                            className={classNames(\"category-name curP mb-10x font-medium fs-15\", {\n                                'text-selected': textSelected[index] \n                            })}\n                            key={index}\n                            onClick={(e) => scrollToView(e,index)}\n                        >\n                            {category.name}\n                        </div>\n                    )\n                })\n            }\n        </div>\n    );\n}\n\nexport default CategoryList;","/Users/baninsenshashrestha/Banin/projects/menu/src/components/FoodItems.js",["83","84"],"import React, {useEffect, useReducer, useState} from 'react';\nimport axios from 'axios';\nimport FoodItem from './FoodItem';\nimport AddOrDeleteItem from './AddOrDeleteItem';\nimport _ from 'lodash';\n\nconst reducer = (state, action) => {\n    switch(action.type) {\n        case 'updateCount': {\n            let arr = (state || []).map((item) => {\n                if (item._id === action.food._id) {\n                    return action.food;\n                }\n                else {\n                    return item;\n                }\n            });\n\n            return arr;\n        }\n        \n        case 'initialize':\n            return [...state, action.food];\n\n        default:\n            return state;\n    }\n}\n\nconst FoodItems = ({setCartItems, categoryList, divRef}) => {\n    const [foodItems, setFoodItems] = useState([]);\n\n    const [foodItemsWithCount, dispatch] = useReducer(reducer, []);\n\n    const dispatchAction = (action) => {\n        dispatch(action);\n    }\n    \n    useEffect(() => {\n        axios.get(\"http://localhost:5000/foods/\")\n            .then(res => {\n                setFoodItems(res.data);\n            })\n    }, []);\n\n    useEffect(() => {\n        let filteredCartItems = foodItemsWithCount.filter((item) => item.count > 0);\n        setCartItems(filteredCartItems);\n    }, [foodItemsWithCount]);\n\n    var groupedFoodItems = _.groupBy(foodItems, \"category_id\");\n\n    return (\n        <div className=\"food-list pl-20x mt-20x\">\n            {\n                (categoryList || []).map((item, index) => {\n                    let categorizedFoodItem = _.find(groupedFoodItems, (value, key) => {\n                        if (key == item._id) return value;\n                    });\n\n                    return (\n                        <div key={index} ref={el => divRef.current[index] = el}>\n                            <div className=\"header-food flex flex-center pt-10x pb-10x mb-15x\">\n                                {item.name}\n                            </div>\n                            {\n                                (categorizedFoodItem || []).map((food, index) => (\n                                    <div className=\"food-item flex flex-between mt-20x pb-10x\" key={index}>\n                                        <FoodItem food={food} />\n                                        <AddOrDeleteItem food={food} dispatchAction={dispatchAction} />\n                                    </div>\n                                ))\n                            }\n                        </div>\n                        \n                    )\n                })\n            }\n        </div>\n    );\n}\n\nexport default FoodItems;","/Users/baninsenshashrestha/Banin/projects/menu/src/components/FoodItem.js",[],"/Users/baninsenshashrestha/Banin/projects/menu/src/components/AddOrDeleteItem.js",["85","86"],"import { faMinus, faPlus } from '@fortawesome/free-solid-svg-icons';\